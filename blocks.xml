            <value name="height">
                <shadow type="math_number">
                    <field name="NUM">60</field>
                </shadow>
            </value>
            <value name="round">
                <shadow type="math_number">
                    <field name="NUM">60</field>
                </shadow>
            </value>
            <value name="angle">
