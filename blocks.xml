    <sep></sep>
    <category id="cathealth" name="Health">
        <block type="MLX90614"></block>
    </category>
    <sep></sep>
    <category id="catRobotics" name="Robotics">
        <block type="QMC5883"></block>
        <block type="otto9_gyro"></block>
