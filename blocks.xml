                </shadow>
            </value>
            <value name="Y">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="width">
                <shadow type="math_number">
                    <field name="NUM">70</field>
                </shadow>
            </value>
            <value name="height">
                <shadow type="math_number">
